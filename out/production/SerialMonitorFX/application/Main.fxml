<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="2.0" vgap="2.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <HBox fx:id="serialPanel" alignment="CENTER_LEFT" minWidth="360.0" prefHeight="25.0" prefWidth="360.0" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
         <children>
            <Button fx:id="serialNamesUpdate" maxHeight="25.0" maxWidth="60.0" minHeight="25.0" minWidth="60.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="Update" />
            <ChoiceBox fx:id="serialNamesChoice" maxHeight="25.0" maxWidth="60.0" minHeight="25.0" minWidth="60.0" prefHeight="25.0" prefWidth="60.0" />
            <Separator orientation="VERTICAL" prefHeight="25.0" prefWidth="8.0" />
            <ChoiceBox fx:id="serialBaudrateChoice" prefHeight="25.0" prefWidth="63.0" />
            <ChoiceBox fx:id="serialEndOfLine" prefHeight="25.0" prefWidth="70.0" />
            <Button fx:id="serialConnectBtn" maxHeight="25.0" maxWidth="70.0" minHeight="25.0" minWidth="70.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Connect" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets></HBox>
      <HBox prefHeight="0.0" prefWidth="358.0" GridPane.rowIndex="1">
         <children>
            <TextField fx:id="serialCmdText" prefHeight="20.0" prefWidth="300.0" />
            <Button fx:id="serialSend" mnemonicParsing="false" prefHeight="20.0" prefWidth="95.0" text="Send" />
         </children>
      </HBox>
      <TextArea fx:id="serialTextBoxRx" prefHeight="380.0" prefWidth="360.0" GridPane.rowIndex="2" />
      <HBox prefHeight="0.0" prefWidth="358.0" spacing="4.0" GridPane.rowIndex="3">
         <children>
            <Button fx:id="serialRxClear" mnemonicParsing="false" prefHeight="25.0" prefWidth="69.0" text="Clear" />
         </children>
      </HBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>

</GridPane>
